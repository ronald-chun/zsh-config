DIR=$(dirname "$0")

# Plugins
source ${DIR}/plugins/zsh-autosuggestions/zsh-autosuggestions.zsh
source ${DIR}/plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

# Themes
source ${DIR}/themes/powerlevel10k/powerlevel10k.zsh-theme
# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ${DIR}/themes/.p10k.zsh ]] || source ${DIR}/themes/.p10k.zsh